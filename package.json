  "scripts": {
    "start": "node src/server",
    "dev": "npm-run-all -p watch-css start-js",
    "prepublish": "NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.config.publish.js && cp ./.npmrc ./build/.npmrc && cp ./CHANGELOG.md ./build/CHANGELOG.md && cp ./README.md ./build/README.md",
    "publish": "cd build && npm publish",
    "start-js": "node scripts/start.js",
    "build-css": "rm -rf ./public/css && ./node_modules/.bin/node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o ./public/css --output-style expanded",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o ./public/css --watch --recursive --output-style expanded",
    "build": "npm-run-all build-css build-js",
    "build-js": "node scripts/build.js",
    "deploy": "sh ./scripts/deploy.sh",
    "test": "node scripts/test.js --env=jsdom",
    "test:unit": "node scripts/test.js --env=jsdom packages/",
    "generate": "./node_modules/.bin/plop --plopfile internals/generators/index.js",
    "lint": "./node_modules/.bin/eslint src/ --fix"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
